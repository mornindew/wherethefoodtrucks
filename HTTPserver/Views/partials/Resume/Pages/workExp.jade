.partialView_Wrapper
	.parialView_PageTitle.
		<i class='fa fa-group pageTitleIcon'></i>  
		&nbsp;
		<span class='hideOnMobile'>Previous</span> Teams 
		&nbsp;
		<i class='fa fa-group pageTitleIcon'></i>  
		
		
	.container
		.col-md-12

		//// Loop through all Past Employers.. 
		for job in jobExp
			//-  PAST WORK EXP ITEM TITLE
			.container.educationItem_titleROW.educationItem_ROW
				if job.icon
					.col-sm-4.col-sm-offset-2
						.educationItem_TITLE.
							#{job.company}
					.col-sm-6
						img(src='#{job.icon}', title='#{job.company}', alt='#{job.company}').tableItemIcon

				else	
					.col-sm-12
						.educationItem_TITLE.
							#{job.company}
			

			//-  Duration ROW
			.container.educationItem_ROW
				.col-sm-6.edu_basicInfoItem.
					Duration : #{job.workDuration}
				.col-sm-6.edu_basicInfoItem.
					Position : #{job.positionTitle}
				
				.col-sm-12
					hr
				.col-sm-2
					.Education_LeftCol.
						Contact
				.col-sm-9
					.container
						.col-sm-3.
							#{job.contact.supervisor}
						.col-sm-1.
							#{job.contact.phone}
						.col-sm-8.
							#{job.contact.webURL}


				.col-sm-12
					hr
				.col-sm-12.
					#{job.contact.address}

			if job.imgs
				.container.educationItem_ROW
					for img in job.imgs
						.col-md-3.col-sm-3.col-xs-6
							//- .imageThumb
							img(src='https://s3-us-west-1.amazonaws.com/adeptpro/Resume/#{img.path}', title='#{img.title}', alt='#{img.title}', data-toggle="tooltip", data-placement="auto right").imageThumb

			.container.educationItem_ROW
				.col-sm-12.
					#{job.description}
			
